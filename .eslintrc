{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "forOf": true,
    "jsx": true,
    "es6": true
  },
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-curly-spacing": 0,
  },
  "plugins": [
    "jsx-a11y"
  ],
  "settings": {
    "import/resolver": "webpack",
  },
}
